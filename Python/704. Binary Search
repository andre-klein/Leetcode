# Solution 1:

class Solution:
    def search(self, nums: List[int], target: int) -> int:
        i = 0
        j = len(nums) - 1

        while j>=i:
            if nums[j] < target or target < nums[i]:
                return -1
            
            m = (i+j)//2
            if target > nums[m]:
                i = m + 1
            elif target < nums[m]:
                j = m - 1
            else:
                return m

# Solution 2:

class Solution:
    def search(self, nums: List[int], target: int) -> int:
        left, right = 0, len(nums) - 1
        while left != right:
            mid = (left + right) // 2
            if nums[mid] < target:
                left = mid + 1
            else:
                right = mid
        return -1 if nums[left] != target else left
