class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        nums_set, cur_max = set(nums), 0

        for num in nums_set:
            if num - 1 not in nums_set:
                length = 1
                
                while num + length in nums_set:
                    length += 1
                
                cur_max = max(cur_max, length)
        
        return cur_max
