class Solution:
    def isValid(self, s: str) -> bool:
        cur = []
        mapping = {')': '(', ']': '[', '}': '{'}
        for char in s:
            if char in mapping:
                if not cur or cur[-1] != mapping[char]:
                    return False
                cur.pop()
            else:
                cur.append(char)
        return True if len(cur) == 0 else False
