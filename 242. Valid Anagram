class Solution:
    def isAnagram(self, s: str, t: str) -> bool:
        dict1 = {}
        
        for char in s:
            if char in dict1:
                dict1[char] += 1
            else:
                dict1[char] = 1

        for char in t:
            if char in dict1:
                dict1[char] -= 1
                if dict1[char] < 0:
                    return False
            else:
                return False

        return max(dict1.values()) == 0

# Time O(n)
# Space O(1)
